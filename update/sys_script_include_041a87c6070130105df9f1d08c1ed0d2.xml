        <name>ValidateEmail</name>
        <script><![CDATA[function ValidateEmail(args1) {

    var validateme = args1;

    if (validateme.includes('@') && validateme.endsWith(".com") && validateme.toString().length > 15) {
        gs.addInfoMessage('Email Validated successfully');
    } 
	else {
        gs.addErrorMessage(('Email is not valid'));
        current.setAbortAction(true);
    }

}]]></script>
